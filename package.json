{
  "name": "@convex/auth",
  "version": "0.0.1",
  "description": "Authentication for Convex",
  "keywords": [
    "authentication",
    "authorization",
    "auth",
    "login",
    "sign",
    "convex"
  ],
  "homepage": "https://labs.convex.dev/auth",
  "bugs": "https://github.com/get-convex/auth/issues",
  "repository": "https://github.com/get-convex/auth",
  "license": "Apache-2.0",
  "author": "xixixao",
  "files": [
    "dist"
  ],
  "type": "module",
  "bin": "./dist/bin.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:bin": "esbuild src/cli/index.ts --bundle --platform=node --outfile=dist/bin.js --external:./node_modules/*",
    "build:server": "tsc --project tsconfig.server.json",
    "build:react": "tsc --project tsconfig.react.json",
    "build": "rm -r dist && npm run build:bin && npm run build:server && npm run build:react",
    "docs": "cd docs && npm run dev",
    "lint": "tsc && eslint . && cd test && npm run lint",
    "prepublishOnly": "npm run build && npm run test:once",
    "test": "cd test && npm run test",
    "test:once": "cd test && npm run test:once"
  },
  "exports": {
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./react": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@auth/core": "^0.31.0",
    "@commander-js/extra-typings": "^12.1.0",
    "arctic": "^1.2.0",
    "jose": "^5.2.2",
    "lucia": "^3.2.0",
    "oslo": "^1.1.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "convex": "^1.11.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "@comment devDependencies": [
    "The dependencies of the CLI are also in devDependencies, built into",
    "a bundle."
  ],
  "devDependencies": {
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.22",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "eslint": "8.49.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  }
}
